package Clock;

public lass Clock {

	private Display hour = new Display(24);
	private Display minute = new Display(60);
	
	public void start()
	{
		minute.increase();
		
		if	(minute.getValue() == 0)
			hour.increase();
	}
	
	public static void main(String[] arg)
	{
		Clock clock = new Clock();
	}
}
